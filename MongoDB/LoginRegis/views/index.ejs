<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Login Registraion</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!---CDN Links-->
    <script src=”//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js”></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" media="screen" href="main.css" />
</head>
<body>
<div id="form">
    <% if(typeof(jserrors) != 'undefined' ) { %>

            <h4>&nbsp;&nbsp;&nbsp;&nbsp;<%= jserrors %></h4>
    <% } %>  
    
    <% if(typeof(errors) != 'undefined' ) { %>
        <% for (var j in errors) { %>
            <h4>&nbsp;&nbsp;&nbsp;&nbsp;<%= errors[j].message %></h4>
        <% } %>
    <% } %>    

    <h1 id="form">Register:</h1>
    <form id="form" action="/register" method="post">
        <div class="form-group">
                <label for="first"> First Name:</label>
                <input type="text" class="form-control" name="first" id="first">
        </div>
        <div class="form-group">
                <label for="last">Last Name:</label><br>
                <input type="text" class="form-control" name="last" id="last">
        </div>   
        <div class="form-group">
                <label for="birthday">Birthday:</label><br>
                <input type="date" class="form-control" name="birthday" id="birthday">
        </div>
        <div class="form-group">
                <label for="email">Email:</label><br>
                <input type="email" class="form-control" name="email" id="email">
        </div> 
        <div class="form-group">
                <label for="password">Password:</label><br>
                <input type="password" class="form-control" name="password" id="password">
        </div> 
        <div class="form-group">
                <label for="cpassword">Confirm Password:</label><br>
                <input type="password" class="form-control" name="cpassword" id="cpassword">
        </div> 
        <button type="submit" class="btn btn-default">Register</button>

    </form> 
    </div> 
    <div id="form">
    
    <h1 id="form">Login:</h1> 
    <form id="form" action="/login" method="post">
        <div class="form-group">
                <label for="logemail">Email:</label><br>
                <input type="email" class="form-control" name="logemail" id="logemail">
        </div> 
        <div class="form-group">
                <label for="logpassword">Password:</label><br>
                <input type="password" class="form-control" name="logpassword" id="logpassword">
        </div>
        <button type="submit" class="btn btn-default">Login</button> 
    </form>
    </div>


</body>
</html>